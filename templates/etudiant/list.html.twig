<ul>
    {% for etudiant in etudiants %}
    <li>
        {{ etudiant|upper }}
    </li>
    {% else %}
    <strong>la liste des etudiants est vide</strong>
    {% endfor %}
</ul>

<h1>Liste des Modules</h1>
<table border="1">
<tr>
    <td>idModule</td>
    <td>Nom</td>
    <td>Enseignant</td>
    <td>Nbre Heures</td>
    <td>Date</td>
</tr>
    {% for m in listModules %}
    <tr>
        <td>{{ m.id }}</td>
        <td>{{ m.nom }}</td>
        <td>{{ m.enseignant }}</td>
        <td>{{ m.nbrHeures }}</td>
        <td>{{ m.date }}</td>
        <td> <a href="{{ path('Affectation') }}">Affecter</a></td>
    </tr>
    {% endfor %}
</table>